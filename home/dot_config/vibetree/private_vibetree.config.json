{
  "cliAutocorrect": true,
  "versionControl": {
    "mergeStrategy": "ask"
  },
  "hooks": {
    "mergePush": true
  },
  "session": {
    "default": "tmux",
    "exitBehavior": "close-switch-any"
  },
  "tool": {
    "codex": {
      "command": [
        "codex"
      ],
      "flags": [
        "--dangerously-bypass-approvals-and-sandbox"
      ],
      "continueFlags": [
        "resume",
        "--last"
      ],
      "promptFlags": []
    },
    "gemini": {
      "command": [
        "gemini"
      ],
      "flags": [
        "--yolo"
      ],
      "continueFlags": [
        "--resume"
      ],
      "promptFlags": false
    },
    "synthetic": {
      "inherit": "claude",
      "command": [
        "synthetic-claude"
      ],
      "description": "Claude with synthetic.new backend"
    }
  }
}
