    "autoPush": true,
    "autoRebase": true,
    "mergeStrategy": "ff-only"
  },
  "tool": {
    "codex": {
      "command": [
        "codex"
      ],
      "flags": [
        "--dangerously-bypass-approvals-and-sandbox"
      ],
      "continueFlags": [
        "resume",
        "--last"
      ],
      "promptFlags": []
    },
    "gemini": {
      "command": [
        "gemini"
      ],
      "flags": [
        "--yolo"
      ],
      "continueFlags": [
        "--resume"
      ],
      "promptFlags": false
    },
    "synthetic": {
      "inherit": "claude",
      "command": [
        "synthetic-claude"
      ],
      "description": "Claude with synthetic.new backend"
    }
  }
}
