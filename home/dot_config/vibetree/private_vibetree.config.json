{
  "$schema": "/Users/bcasey/Projects/brandonocasey/vibetree/dist/vibetree.config.schema.json",
  "hooks": {
    "mergePush": true
  },
  "versionControl": {
    "pushConflictStrategy": "forceWithLease"
  },
  "tool": {
    "codex": {
      "flags": [
        "--dangerously-bypass-approvals-and-sandbox"
      ],
      "continueFlags": [
        "resume",
        "--last"
      ],
      "promptFlags": []
    },
    "gemini": {
      "command": [
        "gemini"
      ],
      "flags": [
        "--yolo"
      ],
      "continueFlags": [
        "--resume"
      ],
      "promptFlags": false
    },
    "synthetic": {
      "inherit": "claude",
      "command": [
        "synthetic-claude"
      ],
      "description": "Claude with synthetic.new backend"
    }
  },
  "smart": {
    "cliAutocorrect": true
  }
}
