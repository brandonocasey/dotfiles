{
  "interface": "ask",
  "cli": {
    "autocorrect": true
  },
  "git": {
    "autoPush": true,
    "autoRebase": true,
    "mergeStrategy": "ff-only"
  },
  "tmux": {
    "rename": true,
    "require": true
  },
  "ai": {
    "gemini": {
      "args": [
        "--yolo"
      ]
    },
    "codex": {
      "args": [
        "-a",
        "never",
        "-s",
        "workspace-write",
        "-c",
        "sandbox_workspace_write.network_access=true",
        "--search"
      ]
    },
    "synthetic": {
      "command": "synthetic-claude",
      "promptArg": "--prompt",
      "continueArg": "--continue",
      "description": "Claude with synthetic.new backend"
    }
  }
}
